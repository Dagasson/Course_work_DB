<Window x:Class="Course_work.Registration"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Course_work"
        mc:Ignorable="d"
        Title="Registration" Height="300" Width="329.323">
    <Grid>
        <TextBox x:Name="login" HorizontalAlignment="Left" Height="23" Margin="80,57,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="160" TextChanged="login_TextChanged"/>
        <TextBox x:Name="password" HorizontalAlignment="Left" Height="23" Margin="80,115,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="160" TextChanged="password_TextChanged"/>
        <Button x:Name="registry" Content="Зарегистрировать" HorizontalAlignment="Left" Margin="80,165,0,0" VerticalAlignment="Top" Width="160" Click="registry_Click"/>

    </Grid>
</Window>
